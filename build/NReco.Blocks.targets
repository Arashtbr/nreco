<!--
Properties: NRecoRepoRootDirectory, NRecoTargetDirectory
-->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<ItemGroup>
		<WebappFiles Include="$(NRecoRepoRootDirectory)\shared\blocks\webapp\**" Exclude="$(NRecoRepoRootDirectory)\shared\blocks\webapp\**\.svn\**"/>
	</ItemGroup>	
	<Target Name="NRecoCopyWebappBlock">
		<Copy
            SourceFiles="@(WebappFiles)"
			SkipUnchangedFiles="true"
            DestinationFiles="@(WebappFiles->'$(NRecoTargetDirectory)\%(RecursiveDir)%(Filename)%(Extension)')"
        />
		
	</Target>

</Project>